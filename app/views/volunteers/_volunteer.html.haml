=content_tag_for(:tr, volunteer) do
  %td= volunteer.name
  - if volunteer.signed_in?
    %td= time_tag volunteer.sign_in_time
    %td= link_to '<i class="icon-signout"></i> Sign Out'.html_safe, sign_out_volunteer_path(volunteer), class: 'btn btn-large btn-success'
  - else
    %td= link_to '<i class="icon-signin"></i> Sign In'.html_safe, sign_in_volunteer_path(volunteer), class: 'btn btn-large btn-warning'

